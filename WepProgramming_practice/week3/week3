//bool
var checkIn = true;
var checkOut = false;
console.log(checkIn);
console.log(checkOut);

var bigger = 100 > 50;
console.log(bigger);
bigger = 100 < 50;
console.log(bigger);

//int
var kuk = 95;
var eng = 85;
var math = 52;
var total = kuk + eng + math;
var avg = total / 3;
console.log(total);
console.log(avg);

var nan = '난'/10;
console.log(nan);

